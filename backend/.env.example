APP_JWT_SECRET=admin
APP_ADMIN_USERNAME=admin
APP_ADMIN_PASSWORD=admin

# e.g. http://localhost:5273 or https://your-domain.com
APP_PUBLIC_BASE_URL=http://localhost:5273

# Comma separated. Empty disables CORS middleware.
APP_CORS_ORIGINS=http://localhost:5273

# Paths are relative to `backend/` when running locally
APP_DB_PATH=./data/app.db
APP_STORAGE_DIR=../uploads

# 1 GiB
APP_MAX_UPLOAD_BYTES=1073741824

# 1 day / 5 minutes
APP_ADMIN_TOKEN_EXPIRES_SECONDS=86400
APP_DOWNLOAD_TOKEN_EXPIRES_SECONDS=300
